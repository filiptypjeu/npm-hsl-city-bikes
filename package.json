{
    "name": "hsl-city-bikes",
    "version": "1.1.0",
    "description": "NPM package for fetching data about city bike station in Helsinki.",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "dev": "nodemon src/index.ts",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "lint-fix": "tslint -p tsconfig.json --fix",
        "postversion": "git push && git push --tags",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "start": "node dist/index.js",
        "test": "jest --config jestconfig.json",
        "version": "npm run format && git add -A src"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:filiptypjeu/npm-hsl-city-bikes.git"
    },
    "homepage": "https://github.com/filiptypjeu/npm-hsl-city-bikes",
    "author": "Filip Stenbacka",
    "license": "ISC",
    "types": "dist/index.d.ts",
    "dependencies": {
        "graphql-request": "^2.0.0",
        "node-fetch": "^2.6.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/node-fetch": "^2.5.10",
        "jest": "^27.0.3",
        "prettier": "^1.19.1",
        "ts-jest": "^27.0.1",
        "ts-node": "^10.0.0",
        "tslint": "^5.20.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.9.9"
    },
    "files": [
        "dist/**/*"
    ],
    "keywords": [
        "hsl",
        "helsinki",
        "city bikes",
        "bikes"
    ]
}
