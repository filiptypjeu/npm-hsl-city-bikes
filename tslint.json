{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "no-empty": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "variable-name": {
            "options": [
                "allow-leading-underscore",
                "ban-keywords",
                "require-const-for-all-caps"
            ]
        }
    }
}